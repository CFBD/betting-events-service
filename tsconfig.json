{
    "compilerOptions": {
      "lib": ["es2023"],
      "target": "ES2022",
      "module": "Node16",
      "moduleResolution": "Node16",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./build",
      "rootDir": "./src",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false
    }
  }
  